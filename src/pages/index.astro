---
import Layout from '../layouts/Layout.astro';
import HeroHome from '../components/hero-home.astro';
import ServicesOverview from '../components/services-overview.astro';
import Locations from '../components/locations.astro';
export const metadata = {
  title: 'Arbo Servis - Rizikové kácení stromů a komplexní péče o dřeviny',
  description:
    'Profesionální kácení rizikových stromů, výškové práce, havarijní kácení, ošetření dřevin, bezpečnostní vazby. Certifikovaní arboristé v Náchodě, Rychnově nad Kněžnou a Hradci Králové. 20 let zkušeností, pojištění, likvidace dřevní hmoty. Zavolejte +420732833212.',
};
---

<Layout title={metadata.title} description={metadata.description}>
  <HeroHome />
  <Locations />
  <ServicesOverview />
</Layout>
