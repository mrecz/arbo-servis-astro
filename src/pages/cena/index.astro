---
import HeroGeneral from '@/components/hero-general.astro';
import Layout from '@/layouts/Layout.astro';
import PriceCalculation from '@/components/price-calculation.astro';
const metadata = {
  title: 'Arbo Servis - Cenová kalkulace',
  description:
